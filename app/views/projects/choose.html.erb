<section class="content-header">
  <h1>
    Project
    <small> <%= @project.name %></small>
  </h1>
  <ol class="breadcrumb">
    <li><a href="#"><i class="fa fa-user"></i> Project</a></li>
    <li class="active"><%= @project.name %></li>
  </ol>
</section>
<section class="content">
  <h1>Projects:</h1>
  <table>
    <tr>
      <th>Title</th>
      <th>Summary</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
   
  <% @projects.each do |project| %>
    <tr>
      <td><%= project.name %></td>
      <td><%= project.description %></td>
      <td><%= link_to "show", project %></td>
      <td><%= link_to "edit", edit_project_path(project) %></td>
      <td><%= link_to "remove", project, method: :delete, data: { confirm: "Do you really want to delete this project?" } %></td>
    </tr>
  <% end %>
  </table>
  <br>
  <%= link_to "New project", new_project_path %>
</section>